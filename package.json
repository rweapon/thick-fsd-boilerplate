{
  "name": "full-fsd-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc && vite build",
    "box-component": "node src/6_shared/model/lib/box-component.mjs",
    "visualize": "rm -rf stats.html && npm run build",
