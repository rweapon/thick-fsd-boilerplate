    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^5.4.8",
